{
  "name": "vue-masterclass-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:init": "supabase init",
    "supabase:login": "supabase login",
    "supabase:link": "dotenv -- cross-var supabase link --project-ref $SUPABASE_PROJECT_REF",
    "db:migrate:new": "supabase migration new",
    "db:reset": "supabase db reset --linked",
    "db:seed": "node --env-file=.env database/seed.js",
    "supabase:types": "dotenv -- cross-var npx supabase gen types --lang=typescript --project-id $SUPABASE_PROJECT_REF --schema public > database/types.ts"
  },
  "dependencies": {
    "@formkit/vue": "^1.6.9",
    "@supabase/supabase-js": "^2.46.1",
    "@tanstack/vue-table": "^8.20.5",
    "@vueuse/core": "^11.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.4.2",
    "iconify-icon": "^2.1.0",
    "lucide-vue-next": "^0.460.0",
    "pinia": "^2.2.4",
    "radix-vue": "^1.9.10",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.13",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.2",
    "supabase": "^1.148.6",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^5.4.10",
    "vue-tsc": "^2.2.0"
  }
}
